<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Contactos Recibidos</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>
<body class="container mt-5">
  <h1 class="mb-4">Lista de Contactos</h1>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Email</th>
        <th>Mensaje</th>
        <th>IP</th>
        <th>Fecha</th>
        <th>Pa√≠s</th>
      </tr>
    </thead>
    <tbody>
      <% contacts.forEach(contact => { %>
        <tr>
          <td><%= contact.name %></td>
          <td><%= contact.email %></td>
          <td><%= contact.message %></td>
          <td><%= contact.ip %></td>
          <td><%= contact.date %></td>
          <td><%= contact.country %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>